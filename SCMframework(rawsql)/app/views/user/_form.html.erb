<div class="card" style="width:100%;">
  <div class="card-header" style="background:#02360B;color:white">
    Register
  </div>
  <div>
<%= form_with url:user_index_path do |form| %>
  <div class = "login">
  <div class="mb-3 row">
  <label for="inputName" class="col-sm-2 col-form-label">Name:</label>
  <div class="col-sm-8">
  <%= form.text_field :name, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputEmail" class="col-sm-2 col-form-label">Email:</label>
  <div class="col-sm-8">
  <%= form.text_field :email, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputPassword" class="col-sm-2 col-form-label">Password:</label>
  <div class="col-sm-8">
  <%= form.password_field :password, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputPasswordConfirmation" class="col-sm-2 col-form-label">Password Confirmation:</label>
  <div class="col-sm-8">
  <%= form.password_field :passwordconfirmation, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputType" class="col-sm-2 col-form-label">Type:</label>
  <div class="col-sm-8">
  <%= form.text_field :user_type, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputPhone" class="col-sm-2 col-form-label">Phone:</label>
  <div class="col-sm-8">
  <%= form.text_field :phone, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputdob" class="col-sm-2 col-form-label">Date of Birth:</label>
  <div class="col-sm-8">
  <%= form.text_field :dob, class: 'form-control' %> 
  </div>
  </div>
  
  <div class="mb-3 row">
  <label for="inputAddress" class="col-sm-2 col-form-label">Address:</label>
  <div class="col-sm-8">
  <%= form.text_field :address, class: 'form-control' %> 
  </div>
  </div>
  <div class="mb-3 row">
  <label for="inputPhoto" class="col-sm-2 col-form-label">Profile:</label>
  <div class="col-sm-8">
  <%= form.file_field :profile, class: 'form-control' %> 
  </div>
  </div>

  <div class = "actions">
    <%= form.submit 'Register', class:'btn btn-primary mb-3'%>
    <%= form.submit 'Clear', class:'btn btn-secondary mb-3'%>
  </div>
  </div>
  <% end %>
  </div>
  </div>