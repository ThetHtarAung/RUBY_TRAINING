<h1>Login</h1>

<%= form_with  method: :post do |f| %>
 
  
  <div class="mb-3">
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>
   
  <div class="mb-3">
    <%= f.label :password %><br/>
    <%= f.password_field :password, class: "form-control" %>
  </div>
   
  <div class="mb-3">
    <%= f.submit "Login", class: "btn btn-primary" %>
  </div>

   
  <% end %>